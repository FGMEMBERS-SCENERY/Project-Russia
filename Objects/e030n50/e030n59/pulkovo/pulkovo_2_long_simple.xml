<?xml version="1.0"?>

<PropertyList>

  <path>pulkovo_2_long_simple.ac</path>

 <animation>
    <type>range</type>
    <min-m>600</min-m>
    <max-m>7000</max-m>
  </animation>  
  
<animation>
    <type>material</type>
    <object-name>oknot</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.65</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.749 </red>
      <green> 0.725 </green>
      <blue>  0.643 </blue>
    </emission>
  </animation>
  
  
  <animation>
    <type>material</type>
    <object-name>oknot</object-name>
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.6</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.0 </red>
      <green> 0.0 </green>
      <blue>  0.0 </blue>
    </emission>
  </animation>

  
</PropertyList>